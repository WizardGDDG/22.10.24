#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "rus");
	int arr[10];
	for (int i = 0; i < 10; i++)
	{
		arr[i] = rand() % 7 + 1;
		cout << arr[i] << " ";
	}
	cout << endl;
	cout << "Цифры которые повторяются: ";
	for (int i = 0; i < 10; i++)
	{
		for (int j = i + 1; j < 10; j++)
		{
			if (arr[j] == arr[i])
			{
				cout << arr[i] << " ";
			}
		}
	}
	
}

